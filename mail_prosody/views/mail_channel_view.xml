<odoo>
    <data>
        <record id="mail_channel_form_view" model="ir.ui.view">
            <field name="name">mail.channel.form</field>
            <field name="model">mail.channel</field>
            <field name="inherit_id" ref="mail.mail_channel_view_form"/>
            <field name="arch" type="xml">
                <field name="description" position="before">
                    <field name="channel_email"/>
                    <field name="is_private"/>
                    <field name="has_password"/>
                    <field name="prosody_room_password"/>
                </field>
            </field>
        </record>
    </data>
</odoo>